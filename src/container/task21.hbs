{{!-- start--------------------------------------------------------------- heading --}}
<div class="card">
	<div class="card-body">
		{{#with heading}}
			{{!-- start--------------------------------------------------------------- main --}}
			{{#with main}}
				<div class="card">
					<div class="card-body">
						<h1 class="heading"> {{title}} </h1>
						<p> {{text}} </p>
						{{#with button_text}}
							<button type="button" class="btn btn-primary">{{solid}}</button>
							<br>
							<br>
							<button type="button" class="btn btn-outline-primary">{{outline}}</button>
						{{/with}}
					</div>
				</div>
			{{/with}}
			{{!-- finish--------------------------------------------------------------- main --}}
			<br>
			{{!-- start--------------------------------------------------------------- second --}}
			{{#with second}}
				<div class="card">
					<div class="card-body">
						<h2 class="heading second"> {{title}} </h2>
						<p> {{text}} </p>
						{{#with button_text}}
							<button type="button" class="btn btn-secondary">{{solid}}</button>
							<br>
							<br>
							<button type="button" class="btn btn-outline-secondary">{{outline}}</button>
						{{/with}}
					</div>
				</div>
			{{/with}}
			{{!-- finish--------------------------------------------------------------- second --}}
			<br>
			{{!-- start--------------------------------------------------------------- sub block --}}
			{{#with subblock}}
				<div class="card">
					<div class="card-body">
						<h3 class="heading subblock"> {{title}} </h3>
						<p> {{text}} </p>
						{{#with button_text}}
							<button type="button" class="btn btn-dark">{{solid}}</button>
							<br>
							<br>
							<button type="button" class="btn btn-outline-dark">{{outline}}</button>
						{{/with}}
					</div>
				</div>
			{{/with}}
			{{!-- finish--------------------------------------------------------------- subblock --}}
		{{/with}}
	</div>
</div>
{{!-- finish--------------------------------------------------------------- heading --}}

<br>

{{!-- start--------------------------------------------------------------- nested_display --}}
<div class="card">
	<div class="card-body">
		{{#with nested_display}}
			<div class="display-1">
				{{title}}
			</div>
			{{!-- start--------------------------------------------------------------- children_display --}}
			<div class="card">
				<div class="card-body">
					{{#with children_display}}
						<div class="display-4">
							{{title}}
						</div>
						<br>
						<p> {{text}} </p>
						{{!-- start--------------------------------------------------------------- button --}}
						{{#with button}}
							<button type="button" class="btn btn-warning"> {{text}} </button>
						{{/with}}
						{{!-- finish--------------------------------------------------------------- button --}}
					{{/with}}
				</div>
			</div>
			{{!-- finish--------------------------------------------------------------- children_display --}}
			<br>
			<h6> {{header_text}} </h6>
			<br>
			{{!-- start--------------------------------------------------------------- button --}}
			{{#with button}}
				<button type="button" class="btn btn-info"> {{text}} </button>
			{{/with}}
			{{!-- finish--------------------------------------------------------------- button --}}
		{{/with}}
	</div>
</div>
{{!-- finish--------------------------------------------------------------- nested_display --}}

<br>

{{!-- start--------------------------------------------------------------- paragraph --}}
<div class="card">
	<div class="card-body">
		{{#with paragraph}}
			<p class="text-uppercase"> {{title}} </p>
			<p> {{text}} </p>
			{{#with button}}
				<a target="_blank"  href="{{url}}" class="btn btn-text-primary"> {{text}} </a>
			{{/with}}
			<br>
			<br>
			<div class="card">
				<div class="card-body">
				{{!-- start--------------------------------------------------------------- list_name --}}
				<ul>
					{{#each list_name}}<br>
						<li> <a target="_blank" href="{{url}}"  class="btn btn-text-primary"> {{text}} </a> </li> 
					{{/each}}
				</ul>
				{{!-- finish--------------------------------------------------------------- list_name --}}
				</div>
			</div>
		{{/with}}
	</div>
</div>
{{!-- finish--------------------------------------------------------------- paragraph --}}




































































